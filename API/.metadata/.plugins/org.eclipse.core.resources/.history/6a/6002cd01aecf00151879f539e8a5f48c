package repository;

import org.springframework.beans.factory.annotation.Qualifier;
import org.springframework.data.repository.CrudRepository;
import org.springframework.stereotype.Repository;

import model.Patient;

@Repository
@Qualifier(value="PatientRepo")
public interface PatientRepo extends CrudRepository<Patient, String> {

	public Patient saveCdoId(int cdoId); 
	public Patient saveName(String name);
	public Patient saveBusinessId(int businessId);
}
