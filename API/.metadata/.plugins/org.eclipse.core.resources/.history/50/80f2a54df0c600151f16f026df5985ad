package model;


@Entity(name="person")
public class Testing {

	private int id;
	private String name;
	
	public Testing(){
		
	}
	
	public int getId(){
		return this.id;
	}
	public String getName(){
		return this.name;
	}
	public void setId(int newId){
		this.id = newId;
	}
	
	public void setName(String newName){
		this.name = newName;
	}
	
}
